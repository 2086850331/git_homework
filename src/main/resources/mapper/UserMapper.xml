<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nf.git.dao.UserDao">

<!--添加-->
    <insert id="add" parameterType="users">
        insert into user_info2(u_id,u_name,age,tel) values(#{uid},#{uname},#{age},#{tel})
    </insert>

<!--    查询-->
    <select id="listCity" parameterType="users" resultType="edu.nf.git.entity.Users">
        select u_id,u_name,age,tel from user_info2
    </select>
</mapper>